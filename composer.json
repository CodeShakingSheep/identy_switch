{
	"name" : "toteph42/identy_switch",
	"type" : "roundcube-plugin",
	"description" : "This plugin allows users to switch between different accounts (and check for new mails) in a single Roundcube session.",
	"homepage" : "https://github.com/totep42/identy_switch",
	"keywords" : [
		"identity",
		"select identy",
		"imap",
		"smtp",
		"mail",
		"switch",
		"new mail check",
		"notify new mail"
	],
	"license" : "GPL-3.0+",
	"authors" : [{
			"name" : "Florian DÃ¤umling",
			"email" : "toteph42@github.com",
			"role" : "Developer"
		}
	],
	"repositories" : [{
			"type" : "composer",
			"url" : "https://plugins.roundcube.net"
		}
	],
	"require" : {
		"php" : ">=8.0.0",
		"roundcube/plugin-installer" : ">=0.1.3",
		"ext-ctype" : "*",
		"boressoft/ident_switch": "*"
	},
	"conflict" : {
		"elm/identity_smtp" : "*",
		"dougluce/ident_switch" : "*"
	},
	"support" : {
		"issues" : "https://github.com/totep42/identy_switch/issues"
	},
	"extra" : {
		"roundcube" : {
			"min-version" : "1.6",
			"sql-dir" : "SQL"
		}
	},
	"config": {
		"allow-plugins": {
			"roundcube/plugin-installer": true
		}
	}
}
